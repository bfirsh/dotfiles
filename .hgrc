[ui]
username = Ben Firshman <ben@firshman.co.uk>
ignore = ~/.hgignore

[extensions]
color = 
fetch = 
progress = 
graphlog = 
hgk = 
hgshelve = ~/.hgext/hgshelve.py

[merge-tools]
threesome.executable = vim
threesome.args = -f $base $local $other $output -c 'ThreesomeInit'
threesome.premerge = keep
threesome.priority = 1

